<?xml version="1.0" encoding="utf-8"?>
<com.ecjia.view.adapter.adaptercell.CategorySellingCell xmlns:android="http://schemas.android.com/apk/res/android"
                                                        android:layout_width="match_parent"
                                                        android:layout_height="wrap_content"
                                                        android:background="@drawable/new_nothing_bg"
                                                        android:gravity="center"
                                                        android:orientation="vertical" >

    <View
        android:layout_width="match_parent"
        android:layout_height="0.6dp"
        android:background="#FFCCCCCC"
        android:visibility="gone" />

    <TextView
        android:id="@+id/good_cell_name_one"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="15dp"
        android:gravity="center_vertical"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="18dp"
         />

    <View
        android:layout_width="match_parent"
        android:layout_height="0.6dp"
        android:background="#FFCCCCCC"
         />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="180dp"
        android:background="@drawable/shape_rectangle_line"
        android:baselineAligned="false"
        android:gravity="center"
        android:orientation="horizontal" >

        <LinearLayout
            android:id="@+id/good_cell_one"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical" >

            <com.ecjia.widgets.webimageview.WebImageView
                android:id="@+id/good_cell_photo_one"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/default_image"
                />
        </LinearLayout>

        <View
            android:layout_width="0.6dp"
            android:layout_height="match_parent"
            android:background="#FFCCCCCC" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/good_cell_two"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="2.5dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/good_cell_name_two"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:maxLines="3"
                        android:textColor="#555555"
                        android:textSize="12dp" />

                    <TextView
                        android:id="@+id/good_cell_price_two"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#ff0000"
                        android:ellipsize="end"
                        android:textSize="12dp" />
                </LinearLayout>

                <com.ecjia.widgets.webimageview.WebImageView
                    android:id="@+id/good_cell_photo_two"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:src="@drawable/default_image"
                    android:scaleType="centerInside" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.6dp"
                android:background="#FFCCCCCC" />

            <LinearLayout
                android:id="@+id/good_cell_three"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="2.5dp"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/good_cell_name_three"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:maxLines="3"
                        android:textColor="#555555"
                        android:ellipsize="end"
                        android:textSize="12dp" />

                    <TextView
                        android:id="@+id/good_cell_price_three"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#ff0000"
                        android:textSize="12dp" />
                </LinearLayout>

                <com.ecjia.widgets.webimageview.WebImageView
                    android:id="@+id/good_cell_photo_three"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:src="@drawable/default_image"
                    android:scaleType="centerInside" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.6dp"
        android:background="#FFCCCCCC"
        />
    <View
        android:id="@+id/category_foot_view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/default_new_distance"
        android:background="#FFFFFF" />
    <RelativeLayout
        android:id="@+id/category_suggest_title"
        android:layout_width="match_parent"
        android:background="#ededed"
        android:layout_height="45dp">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true">

            <View
                android:layout_width="match_parent"
                android:layout_height="0.6dp"
                android:layout_marginRight="15dp"
                android:layout_marginLeft="15dp"
                android:background="@color/filter_text_color" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="#ededed"
            android:layout_centerInParent="true">


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginRight="15dp"
                android:layout_marginLeft="15dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:src="@drawable/suggest_down" />

                <TextView
                    android:id="@+id/tv_suggest"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:text="为你推荐"
                    android:gravity="center_vertical"
                    android:textColor="@color/filter_text_color" />
            </LinearLayout>

        </FrameLayout>

    </RelativeLayout>

</com.ecjia.view.adapter.adaptercell.CategorySellingCell>